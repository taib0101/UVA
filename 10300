#include<bits/stdc++.h>
#define ll long long
using namespace std;

int main()
{
    int a,b,c,t,n,sum;
    cin>>t;

    while(t--)
    {
        sum = 0;
        cin>>n;
        for(int i=1; i<=n; ++i)
        {
            cin>>a>>b>>c;

            sum = sum + (a*c);
        }

        cout<<sum<<endl;
    }
    return 0;
}
