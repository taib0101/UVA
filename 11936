#include<bits/stdc++.h>
using namespace std;
int main()
{
//  freopen("input.txt","r",stdin);
//   freopen("output.txt","w",stdout);
    int t;
    cin>>t;
    while(t--)
    {
        int a,b,c,flag = 0;
        cin>>a>>b>>c;

        if(a + b > c && b + c > a && c + a > b)
            cout<<"OK\n";
        else
            cout<<"Wrong!!\n";
    }
    return 0;
}
