#include<bits/stdc++.h>
using namespace std;

int main()
{
    int a,b,c,x,y;

    while(cin>>a>>b>>c && (a,b,c)>0)
    {
        if(a > c)
            swap(a,c);
        if(b > c)
            swap(b,c);
       // cout<<"a = "<<a<<",b = "<<b<<",c = "<<c<<endl;
        x = a*a;
        y= b*b;

        if((x+y) == (c*c))
            cout<<"right\n";
        else
            cout<<"wrong\n";
    }
    return 0;
}
