#include<bits/stdc++.h>
using namespace std;

int main()
{
//    freopen("input.txt","r",stdin);
//    freopen("output.txt","w",stdout);
    int r, n, i = 1;
    while(cin >> r >> n && r && n)
    {
        cout << "Case " << i << ": ";
        if(r / n <= 26)
        {
            if(r % n == 0)
                cout << (r / n) - 1 << endl;
            else
                cout << r / n << endl;
        }
        else
            cout << "impossible" << endl;
        i++;
    }
    return 0;
}
