#include<bits/stdc++.h>
using namespace std;

int main()
{
    int t;
    cin>>t;
    while(t--)
    {
        int s,b;
        cin>>s>>b;
        int x = s + b, y = abs(s - b);

        if(x % 2 == 0 && y % 2 == 0 && s >= b)
            cout<<x/2<<" "<<y/2<<endl;
        else
            cout<<"impossible\n";
    }
    return 0;
}
