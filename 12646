#include<bits/stdc++.h>
using namespace std;

int main()
{
    int a, b, c;
    while(cin >> a >> b >> c)
    {
        int flag = 0;
        char x = '*';
        if(a == b && a != c)
            x = 'C';
        else if(b == c && b != a)
            x = 'A';
        else if(c == a && c != b)
            x = 'B';
        cout << x << endl;
    }
    return 0;
}
