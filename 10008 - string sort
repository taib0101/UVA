#include<bits/stdc++.h>
using namespace std;

struct obj
{
    char cc;
    int a;
};
bool cmp (struct obj x, struct obj y)
{
    if(x.a != y.a)
        return x.a > y.a;
    return x.cc < y .cc;
}

int main()
{
    int t;
    string str;
    cin >> t;
    getline(cin, str);
    map < char, int > mp;
    map < char, int > :: iterator it;

    obj arr[30];
    int k = 0, arrr[200] = {0};
    while(t--)
    {
        getline(cin, str);
        for(int i = 0; i < str.size(); ++i)
        {


            if(isalpha(str[i]))
            {
                if(islower(str[i]))
                  str[i] = toupper(str[i]);
                if(mp.find(str[i]) == mp.end())
                    mp[str[i]] = k++;
                if(isupper(str[i]))
                {
                    it = mp.find(str[i]);
                    arr[it->second].cc = str[i];
                    arr[it->second].a = ++arrr[str[i]];
                }
                else
                {

                    it = mp.find(str[i]);
                    arr[it->second].cc = str[i];
                    arr[it->second].a = ++arrr[str[i]];
                }
            }
        }
    }
    sort(arr, arr+k, cmp);
    for(int i = 0; i < k; ++i)
        cout << arr[i].cc << " " << arr[i].a << endl;
    return 0;
}
