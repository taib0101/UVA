#include<bits/stdc++.h>
using namespace std;

int main()
{
//    freopen("input.txt","r",stdin);
//    freopen("output.txt","w",stdout);
    int t, to = 0, co;
    cin >> t;
    while(t--)
    {
        double l, w, d, sum = 0, we;
        cin >> l >> w >> d >> we;
        co = 1;
        sum = l + w + d;
        if(sum <= 125.00 && we <= 7.00)
            co = 1;
        else if(l > 56.00 || w > 45.00 || d > 25.00 || we > 7.00)
            co = 0;
        cout << co << endl;
        to += co;
    }
    cout << to << endl;
    return 0;
}
