#include<bits/stdc++.h>
using namespace std;

int main()
{
//    freopen("input.txt","r",stdin);
//    freopen("output.txt","w",stdout);

    int t;
    cin>>t;
    while(t--)
    {
        int n,m;
        cin>>n>>m;
        for(int i=1; i<=m; ++i)
        {
            for(int j=1; j<=n; ++j)
            {
                for(int k=1; k<=j; ++k)
                    cout<<j;
                cout<<endl;
            }
            for(int j=n-1; j>=1; --j)
            {
                for(int k=1; k<=j; ++k)
                    cout<<j;
                cout<<endl;
            }
           if(m != i || t != 0) cout<<endl;
        }
    }
    return 0;
}


